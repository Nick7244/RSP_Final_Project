<launch>

  <node name="gazebo"
	pkg="gazebo_ros"
	type="gazebo"
	args="$(find kendama_gazebo)/worlds/kendama.world">
  </node>

  <include file="$(find kendama_description)/launch/kendama_startup.launch"/>

  <node name="spawn_model"
	pkg="gazebo_ros"
	type="spawn_model"
	args="-urdf -param robot_description -model kendama">
  </node>

  <!--
  <node name="rviz"
	pkg="rviz"
	type="rviz"
	args="-d $(find edumip_gazebo)/kendama_config.rviz">
  </node>
  -->

</launch>
