// Ball tracker node

#include <kendama_visp_tracker/color_sub.hpp>
#include <kendama_visp_tracker/depth_sub.hpp>

int main(int argc, char** argv) {

  ros::init(argc, argv, "ball_tracker");
  ros::NodeHandle nh_color;
  ros::NodeHandle nh_depth;

  color_sub color_subscriber(nh_color);
  depth_sub depth_subscriber(nh_depth);

  ros::spin();
  return 0;

}
